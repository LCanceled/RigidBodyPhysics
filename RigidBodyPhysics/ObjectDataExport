
import bpy

out = open('C:\\Users\\Aric\\Documents\\Visual Studio 2013\\Projects\\RigidBodyPhysics\\RigidBodyPhysics\\tank_scene_info.txt', 'w')

for ob in bpy.data.objects:
    if (ob.type == 'MESH'):
        quat = ob.rotation_euler.to_quaternion()
        out.write('%s %f %f %f %f %f %f %f\n' % (ob.name, ob.location.x, ob.location.z, ob.location.y, quat.x, quat.z, quat.y, quat.w))
out.close()